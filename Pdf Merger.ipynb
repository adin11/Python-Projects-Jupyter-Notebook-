   "source": [
    "import PyPDF2\n",
    "\n",
    "merger = PyPDF2.PdfMerger()\n",
    "pdffiles = ['1.pdf','2.pdf','3.pdf']\n",
    "\n",
    "\n",
    "for file in pdffiles:\n",
    "    pdffile= open(file,'rb')\n",
    "    reader_object = PyPDF2.PdfReader(pdffile)\n",
    "    merger.append(reader_object)\n",
    "\n",
    "pdffile.close()\n",
    "merger.write(\"Merged Pdf\")\n",
    "    \n"
   ]
